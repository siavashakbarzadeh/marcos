<!DOCTYPE html>
<html lang="en" dir="rtl">

<head>
  
  <meta charset="utf-8" />
  <title><%- title %> | Minia - Minimal Admin & Dashboard Template</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <meta content="Premium Multipurpose Admin & Dashboard Template" name="description">
  <meta content="Themesbrand" name="author">
  <!-- App favicon -->
  <link rel="shortcut icon" href="public/assets/images/favicon.ico">

  <base href="/">

  <%- HeaderCss %>
  
<!-- preloader css -->
<link rel="stylesheet" href="public/assets/css/preloader.min.css" type="text/css" />

<!-- Bootstrap Css -->
<link rel="stylesheet" type="text/css" href="public/assets/css/bootstrap-rtl.min.css" id="bootstrap-style" />
<!-- Icons Css -->
<link rel="stylesheet" type="text/css" href="public/assets/css/icons.min.css" />
<!-- App Css-->
<link rel="stylesheet" type="text/css" href="public/assets/css/app-rtl.min.css" id="app-style" />

</head>

<body>

  <!-- Begin page -->
  <div id="layout-wrapper">

    <%- include ('./Partials/Header') %>
    <%- include ('./Partials/Sidebar') %>

    <div class="main-content">
      <div class="page-content">
        <div class="container-fluid">
          <%- breadcrumb %>
          
          <%- body %>
        </div>
      </div>
      <%- include ('./Partials/Footer') %>
    </div>
  </div>

  <%- include ('./Partials/Rightbar')  %>
  <%- include ('./Partials/VendorScript')  %>

  <%- FooterJs %>

  <script src="public/assets/js/app.js"></script>

  <%- BottomJs %>
</body>

</html>